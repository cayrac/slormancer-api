{
    "name": "slormancer-api",
    "version": "0.2.3",
    "description": "A Slormancer api to parse saves",
    "keywords": ["slormancer"],
    "author": "cayrac",
    "license": "ISC",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "umd:main": "dist/umd/index.js",
    "types": "dist/types/index.d.js",
    "files": [
        "dist"
    ],
    "homepage": "https://github.com/cayrac/slormancer-api#readme",
    "bugs": {
        "url": "https://github.com/cayrac/slormancer-api/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cayrac/slormancer-api.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "clean": "if exist dist del /s /q dist && if exist slormancer-api-*.tgz del slormancer-api-*.tgz",
        "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
        "build:cjs": "tsc -p config/tsconfig.cjs.json",
        "build:esm": "tsc -p config/tsconfig.esm.json",
        "build:umd": "webpack --config config/webpack.config.js",
        "build:types": "tsc -p config/tsconfig.types.json",
        "pack": "npm run build && npm pack"
    },
    "devDependencies": {
        "@angular/core": "14.2.4",
        "@types/node": "18.11.6",
        "eslint": "8.26.0",
        "typescript": "4.8.4",
        "webpack": "5.74.0",
        "webpack-cli": "4.10.0",
        "ts-loader": "9.4.1",
        "rxjs": "7.5.7"
    }
}