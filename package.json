{
    "name": "slormancer-api",
    "version": "1.0.0",
    "description": "A Slormancer api to parse saves",
    "keywords": ["slormancer"],
    "author": "cayrac",
    "license": "ISC",
    "homepage": "https://github.com/cayrac/slormancer-api#readme",
    "bugs": {
        "url": "https://github.com/cayrac/slormancer-api/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cayrac/slormancer-api.git"
    },
    "scripts": {
        "ng": "ng",
        "build": "npm run build:esm && npm run build:cjs",
        "build:esm": "tsc -p ./tsconfig.esm.json && move lib/esm/index.js lib/esm/index.mjs",
        "build:cjs": "tsc -p ./tsconfig.cjs.json",
        "prepack": "npm run build"
    },
    "dependencies": {
        "node": "18.10.0"
    },
    "devDependencies": {
        "@angular/compiler-cli": "14.2.4",
        "@angular-eslint/schematics": "14.1.2",
        "@types/node": "18.8.2",
        "eslint": "8.24.0",
        "typescript": "4.8.4"
    },
    "files": [
        "lib/**/*"
    ],
    "types": "./lib/cjs/types/index.d.ts",
    "main": "./lib/cjs/index.js",
    "exports": {
        ".": {
            "import": {
                "types": "./lib/esm/types/index.d.ts",
                "default": "./lib/esm/index.mjs"
            },
            "require": {
                "types": "./lib/cjs/types/index.d.ts",
                "default": "./lib/cjs/index.js"
            }
        }
    }
}